<h1>Listado de Usuarios</h1>
<%= link_to "Crear Usuario", new_user_path %>
<table>
  <tr>
    <thead>
      <th>Nombre</th>
      <th>Email</th>
    </thead>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td>
      <%= link_to "Editar", edit_user_path(user) %>
      <%= link_to "Eliminar", user, method: :delete, data: { confirm: "Desea eliminar este usuario?"} %>
    </td>
  </tr>
  <% end %>
</table>